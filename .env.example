# VidGro Admin Panel - Environment Template
# Copy this file to .env.local for development or .env.production for production
# Replace placeholder values with your actual configuration

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
VITE_SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Database direct connection (for Node.js scripts)
DATABASE_URL=postgresql://username:password@host:port/database

# =============================================================================
# ADMIN CONFIGURATION
# =============================================================================
VITE_ADMIN_EMAIL=admin@vidgro.com
VITE_ADMIN_SECRET_KEY=your_admin_secret_key

# =============================================================================
# APPLICATION CONFIGURATION
# =============================================================================
VITE_APP_NAME=VidGro Admin Panel
VITE_API_BASE_URL=https://your-api-domain.com

# =============================================================================
# ADMOB CONFIGURATION
# =============================================================================
VITE_ADMOB_APP_ID=your_admob_app_id
VITE_ADMOB_BANNER_ID=your_admob_banner_id
VITE_ADMOB_INTERSTITIAL_ID=your_admob_interstitial_id
VITE_ADMOB_REWARDED_ID=your_admob_rewarded_id

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================
VITE_JWT_SECRET=your_jwt_secret_key

# =============================================================================
# ENVIRONMENT CONFIGURATION
# =============================================================================
NODE_ENV=development
VITE_NODE_ENV=development

# =============================================================================
# MOBILE CLIENT CONFIGURATION
# =============================================================================
MOBILE_CLIENT_ID=your_mobile_client_id
MOBILE_CLIENT_SECRET=your_mobile_client_secret

# =============================================================================
# BACKUP CONFIGURATION
# =============================================================================
SUPABASE_BACKUP_BUCKET=your_backup_bucket